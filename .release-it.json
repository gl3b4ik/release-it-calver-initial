{
  "git": {
    "requireBranch": "main",
    "commitMessage": "chore(release): ${version}"
  },
  "npm": false,
  "github": {
    "release": true
  },
  "plugins": {
    "@csmith/release-it-calver-plugin": {
      "format": "yyyy.mm.minor",
      "increment": "calendar.minor"
    }
  },
  "verbose": 2
}
